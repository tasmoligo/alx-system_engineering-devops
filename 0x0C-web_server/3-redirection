#!/usr/bin/env bash
# Installs nginx server
sudo apt-get update
sudo apt-get -y install nginx
sudo echo "Hello World!" > /var/www/html/index.html
str1="server_name _;\n\trewrite ^/(.*)$ https://github.com/tasmoligo permanent"
sudo sed -i 's/server_name;/$str1/' /etc/nginx/sites-enabled/default
sudo service nginx start
